[Unit]
Description=AgentTinder Telegram Chat ID Detector (one-shot)
After=network.target

[Service]
Type=oneshot
User=ubuntu
WorkingDirectory=/home/ubuntu/Agent-Tinder
ExecStart=/usr/bin/bash /home/ubuntu/Agent-Tinder/ops/scripts/telegram_chat_detector.sh
EnvironmentFile=/home/ubuntu/Agent-Tinder/.env

[Install]
WantedBy=multi-user.target
